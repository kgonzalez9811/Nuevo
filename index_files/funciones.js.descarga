    if (pag === undefined) {
        pag = 1;
    }
    //    console.log(pag);

    var array;
    var bg_color;
    $(".search-results").empty();
    for (var i = 0; i < $TAGS_ARRAY.length; i++) {
        if ($TAGS_ARRAY[i]['color']) {
            // bg_color = 'style="background-color:'+$TAGS_ARRAY[i]['color']+';"';
