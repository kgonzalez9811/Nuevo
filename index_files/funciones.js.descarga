function format_rut() {
    $("input[name='rut']")
        .rut({ formatOn: 'keyup', validateOn: 'keyup' })
        .on('rutInvalido', function() {
            $(this).parents(".rut").addClass("error");
        })
        .on('rutValido', function() {
            $(this).parents(".rut").removeClass("error");
        });
}
